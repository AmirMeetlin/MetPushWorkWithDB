        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetPushWorkWithDB.Windows"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <StackPanel HorizontalAlignment="Center"
                VerticalAlignment="Center">
            <TextBlock 
                
                FontSize="35"
                x:Name="tbAdminInfo"
                >

            </TextBlock>
            <Button
            Width="200"
            Height="24"
            Content="Просмотреть всех пользователей"
                x:Name="btnViewUsers" Click="btnViewUsers_Click"
                >
                
            </Button>
        </StackPanel>
    </Grid>
</Window>
